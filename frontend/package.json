{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run start:nest\"",
    "dev:next": "next dev",
    "start:dev": "nest start --watch",
    "start:nest": "cd src && npm run start:dev",
